<div class="{{namespace}} info-win-dialog">
	<p class="title">{{classification}}</p>
	<div class="content">
		<div class="infoDiv">
			<div class="imgDiv">
				<img src="{{thumbnail}}" alt="no picture"><br>
			</div>
			<div class="exportFeature">
				<button type="submit" class="btn btn-primary active">Export Feature</button>
			</div>
		</div>
		<div class="tableDiv">
			<table class="table table-striped">
				<tbody>
					{{#each attributes}}
						<tr>
							<td>{{@key}}</td>
							<td>{{{this}}}</td>
						</tr>
					{{/each}}
				</tbody>
			</table>
		</div>
	</div>
</div>