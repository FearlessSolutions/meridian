<div class="modal fade" id="data-history-modal" tabindex="-1" role="dialog" aria-labelledby="data-history-modal" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title">Data History<span class="data-history-modal-back-to-list pull-right hidden"><span class="glyphicon glyphicon-arrow-left"></span></span></h4>
            </div>
            <div class="modal-body">
                <ul class="data-history-list">
                    <li class="history-record">
                        <span class="data-name">My Query</span>
                        <span class="data-date">Mon Sept 8, 2014 9:15am</span>
                        <span class="data-record-count">355 features</span>
                        <span class="data-actions">
                            <button type="button" class="btn btn-xs btn-default data-action-info">i</button>
                            <button type="button" class="btn btn-xs btn-default data-action-restore">+</button>
                            <button type="button" class="btn btn-xs btn-default data-action-delete">x</button>
                        </span>
                    </li>
                    <li class="history-record">
                        <span class="data-name">Another Query</span>
                        <span class="data-date">Mon Sept 8, 2014 2:23pm</span>
                        <span class="data-record-count">1,245 features</span>
                        <span class="data-actions">
                            <button type="button" class="btn btn-xs btn-default data-action-info">i</button>
                            <button type="button" class="btn btn-xs btn-default data-action-restore">+</button>
                            <button type="button" class="btn btn-xs btn-default data-action-delete">x</button>
                        </span>
                    </li>
                    <li class="history-record">
                        <span class="data-name">Some Very Long Query Name</span>
                        <span class="data-date">Mon Sept 8, 2014 8:22am</span>
                        <span class="data-record-count">12,987 features</span>
                        <span class="data-actions">
                            <button type="button" class="btn btn-xs btn-default data-action-info">i</button>
                            <button type="button" class="btn btn-xs btn-default data-action-restore">+</button>
                            <button type="button" class="btn btn-xs btn-default data-action-delete">x</button>
                        </span>
                    </li>
                </ul>
                <div class="data-history-detail-view hidden">
                    <div class="pull-right">
                        <img src="http://www.adventistdirectory.org/images2012/map-world.png" width="200">
                        <span class="data-actions">
                            <button type="button" class="btn btn-sm btn-default data-action-info">i</button>
                            <button type="button" class="btn btn-sm btn-default data-action-restore">+</button>
                            <button type="button" class="btn btn-sm btn-default data-action-delete">x</button>
                        </span>
                    </div>
                    <div class="meta">
                        <span>{queryName}</span>
                        <span>{datasetId}</span>
                        <span>{date}</span>
                        <span>{count}</span>
                        <span>{extent}</span>
                        <span>{createdOn}</span>
                        <span>{dataService}</span>
                        <span>{justification}</span>
                        <span>{auth}</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div